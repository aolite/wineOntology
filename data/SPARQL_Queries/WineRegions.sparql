PREFIX geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>
PREFIX dbp: <http://dbpedia.org/property/>
PREFIX db: <http://dbpedia.org/>
PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX dbc: <http://dbpedia.org/resource/Category:>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT *
WHERE {
  ?wineRegion dct:subject dbc:Wine_regions_of_Spain .
  ?wineRegion dbo:wikiPageID ?id .
  ?wineRegion dbp:wineries ?num_wineries .
  ?wineRegion dbp:wineProduced ?wine_produced .
  ?wineRegion dbp:name ?wine_region_name .
  ?wineRegion dbp:officialName ?wine_region_official_name .
  ?wineRegion dbp:year ?wine_region_year .
  OPTIONAL {?wineRegion geo:lat ?wine_region_lat .}
  OPTIONAL {?wineRegion geo:long ?wine_region_long .}
}