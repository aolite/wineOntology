@prefix : <https://w3id.org/winecatalogue#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/winecatalogue#> .

<https://w3id.org/winecatalogue> rdf:type owl:Ontology ;
                                  owl:versionIRI <https://w3id.org/winecatalogue/1.0.0> ;
                                  <http://purl.org/dc/elements/1.1/abstract> "The domain of the ontology is framed around the classification of wines and their main characteristics. The scope of the ontology will be modelled on the characteristics of wine as well as the regions to which they belong."@en ;
                                  <http://purl.org/dc/elements/1.1/description> "An ontology devoted to describe the spanish wine characteristics and prices."@en ;
                                  <http://purl.org/dc/elements/1.1/title> "Wine Cataloguing Ontology"@en ;
                                  owl:versionIRI "https://w3id.org/winecatalogue/1.0.0"@en ;
                                  owl:versionInfo """Initial version of the ontology with the incorporation of the following datasets: 

- https://www.kaggle.com/datasets/fedesoriano/spanish-wine-quality-dataset 

- https://dbpedia.org/page/Category:Wine_regions_of_Spain

- https://dbpedia.org/page/Category:Wineries_of_Spain"""@en ;
                                  <https://www.dublincore.org/specifications/bibo/bibo/bibo.rdf.xml#status> "Draft"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/abstract
<http://purl.org/dc/elements/1.1/abstract> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionIRI
owl:versionIRI rdf:type owl:AnnotationProperty .


###  https://www.dublincore.org/specifications/bibo/bibo/bibo.rdf.xml#status
<https://www.dublincore.org/specifications/bibo/bibo/bibo.rdf.xml#status> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://dbpedia.org/ontology/location
<http://dbpedia.org/ontology/location> rdf:type owl:ObjectProperty ;
                                       rdfs:range <http://dbpedia.org/ontology/Country> ;
                                       rdfs:comment "An object property that represents the country of a wine"@en ;
                                       rdfs:isDefinedBy "http://dbpedia.org/ontology/"@en ;
                                       rdfs:label "location"@en .


###  https://saref.etsi.org/core/unitOfMeasure
<https://saref.etsi.org/core/unitOfMeasure> rdf:type owl:ObjectProperty ;
                                            rdfs:comment "An object property that facilitate the specification of unit of measure for a Measurement"@en ;
                                            rdfs:isDefinedBy "https://saref.etsi.org/core/"@en ;
                                            rdfs:label "unit of measure"@en .


###  https://w3id.org/winecatalogue#hasPrice
:hasPrice rdf:type owl:ObjectProperty ;
          rdfs:comment "An object property that identify a price for a specific wine."@en ;
          rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
          rdfs:label "has price"@en .


###  https://w3id.org/winecatalogue#hasRegion
:hasRegion rdf:type owl:ObjectProperty ;
           rdfs:comment "An object property that permit to define a wine region for an specific wine"@en ;
           rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
           rdfs:label "has region"@en .


###  https://w3id.org/winecatalogue#hasWine
:hasWine rdf:type owl:ObjectProperty ;
         owl:inverseOf :isWineOf ;
         rdfs:domain <http://schema.org/Winery> ;
         rdfs:range :Wine ;
         rdfs:comment "An object property that represents the Wines that a Winery have."@en ;
         rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
         rdfs:label "has Wine"@en ;
         rdfs:seeAlso "https://w3id.org/winecatalogue#isWineOf"@en .


###  https://w3id.org/winecatalogue#hasWineDescriptor
:hasWineDescriptor rdf:type owl:ObjectProperty ;
                   rdfs:range :WineDescriptor ;
                   rdfs:comment "An object property that details the wine characterstics according to the flavour and body"@en ;
                   rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
                   rdfs:label "has wine descriptor"@en .


###  https://w3id.org/winecatalogue#hasWineQuality
:hasWineQuality rdf:type owl:ObjectProperty ;
                rdfs:comment "An object property that identifies wine quality to an specific wine"@en ;
                rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
                rdfs:label "has wine quality"@en .


###  https://w3id.org/winecatalogue#hasWineType
:hasWineType rdf:type owl:ObjectProperty ;
             rdfs:comment "An object property that identifies the wine variety to an specific wine"@en ;
             rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
             rdfs:label "has wine type"@en .


###  https://w3id.org/winecatalogue#isWineOf
:isWineOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:comment "An object property that identifies the winery of an specific wine."@en ;
          rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
          rdfs:label "is wine of"@en ;
          rdfs:seeAlso "https://w3id.org/winecatalogue#hasWine"@en .


#################################################################
#    Data properties
#################################################################

###  http://dbpedia.org/ontology/wikiPageID
<http://dbpedia.org/ontology/wikiPageID> rdf:type owl:DatatypeProperty ;
                                         rdfs:subPropertyOf owl:topDataProperty ;
                                         rdfs:range xsd:integer ;
                                         rdfs:comment "Internal Wiki Page ID for DBpedia"@en ;
                                         rdfs:isDefinedBy "http://dbpedia.org/ontology/"@en ;
                                         rdfs:label "wiki page ID"@en .


###  http://dbpedia.org/property/officialName
<http://dbpedia.org/property/officialName> rdf:type owl:DatatypeProperty ,
                                                    owl:FunctionalProperty ;
                                           rdfs:range rdfs:Literal ,
                                                      xsd:string ;
                                           rdfs:comment "A data property that refers to the official name of an entity"@en ;
                                           rdfs:isDefinedBy "http://dbpedia.org/property/"@en ;
                                           rdfs:label "official name"@en .


###  http://dbpedia.org/property/year
<http://dbpedia.org/property/year> rdf:type owl:DatatypeProperty ;
                                   rdfs:range xsd:integer ;
                                   rdfs:comment "A data property that permit to identify the year for an entity"@en ;
                                   rdfs:isDefinedBy "http://dbpedia.org/property/"@en ;
                                   rdfs:label "year"@en .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
<http://www.w3.org/2003/01/geo/wgs84_pos#lat> rdf:type owl:DatatypeProperty ;
                                              rdfs:subPropertyOf owl:topDataProperty ;
                                              rdfs:range xsd:float ;
                                              rdfs:comment "A data property that permits to identofy the latiture according to the WGS84"@en ;
                                              rdfs:isDefinedBy "http://www.w3.org/2003/01/geo/wgs84_pos#"@en ;
                                              rdfs:label "latitude"@en .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
<http://www.w3.org/2003/01/geo/wgs84_pos#long> rdf:type owl:DatatypeProperty ;
                                               rdfs:range xsd:float ;
                                               rdfs:comment "A data property that permit to identify a longitude according to WGS84"@en ;
                                               rdfs:isDefinedBy "http://www.w3.org/2003/01/geo/wgs84_pos#"@en ;
                                               rdfs:label "longitude"@en .


###  https://w3id.org/winecatalogue#acidity
:acidity rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:integer ;
         rdfs:comment "A data property that represents the acidity score, defined as wine's “pucker” or tartness; it's what makes a wine refreshing and your tongue salivate and want another sip [from 1-5]"@en ;
         rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
         rdfs:label "acidity"@en .


###  https://w3id.org/winecatalogue#bodyScore
:bodyScore rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:double ;
           rdfs:comment "A data property that represents the richness and weight of the wine in your mouth [from 1-5]"@en ;
           rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
           rdfs:label "body score"@en .


###  https://w3id.org/winecatalogue#hasName
:hasName rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:range rdfs:Literal ,
                    xsd:string ;
         rdfs:comment "Data property that represents the name of an entity."@en ;
         rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
         rdfs:label "has name"@en .


###  https://w3id.org/winecatalogue#hasValue
:hasValue rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:float ;
          rdfs:comment "A data property that refers the specific value for a measurement or currency"@en ;
          rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
          rdfs:label "has value"@en .


###  https://w3id.org/winecatalogue#id
:id rdf:type owl:DatatypeProperty ,
             owl:FunctionalProperty ;
    rdfs:range xsd:string ;
    rdfs:comment "A data property that permit to define an id for an entity"@en ;
    rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
    rdfs:label "id"@en .


###  https://w3id.org/winecatalogue#numReviews
:numReviews rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:range xsd:integer ;
            rdfs:comment "Number of users that reviewed the wine"@en ;
            rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
            rdfs:label "number of reviews"@en .


###  https://w3id.org/winecatalogue#rating
:rating rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:integer ;
        rdfs:comment "A data property that represents the average rating given to the wine by the users [from 1-5]"@en ;
        rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
        rdfs:label "rating"@en .


###  https://w3id.org/winecatalogue#wineProduced
:wineProduced rdf:type owl:DatatypeProperty ;
              rdfs:comment "A data property that reflects the quantity of wine produced in a region."@en ;
              rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
              rdfs:label "wine produced"@en .


###  https://w3id.org/winecatalogue#wineries
:wineries rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:range xsd:integer ;
          rdfs:comment "A data property to define the number of wineries for a region"@en ;
          rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
          rdfs:label "wineries"@en .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Country
<http://dbpedia.org/ontology/Country> rdf:type owl:Class ;
                                      rdfs:comment "A country is a distinct part of the world, such as a state, nation, or other political entity. It may be a sovereign state or make up one part of a larger state"@en ;
                                      rdfs:isDefinedBy "http://dbpedia.org/ontology/"@en ;
                                      rdfs:label "Country"@en .


###  http://dbpedia.org/ontology/Wine
<http://dbpedia.org/ontology/Wine> rdf:type owl:Class ;
                                   rdfs:comment "Wine (from the Latin vinum) is a beverage made from grapes (Vitis vinifera species), through the alcoholic fermentation of their must or juice. Fermentation is produced by the metabolic action of yeasts, which transform the natural sugars of the fruit into ethanol and gas in the form of carbon dioxide. The sugar and acids in the fruit, Vitis vinifera, are sufficient for fermentation to take place. However, wine is a sum of environmental factors: climate, latitude, altitude, hours of light and temperature, among others. Approximately 66 % of the world's grape harvest is dedicated to wine production; the rest is for consumption as fruit. Despite this, vine cultivation covers only 0.5 % of the world's arable land."@en ;
                                   rdfs:isDefinedBy "http://dbpedia.org/ontology/"@en ;
                                   rdfs:label "Wine"@en .


###  http://dbpedia.org/resource/Category:WineRegion
<http://dbpedia.org/resource/Category:WineRegion> rdf:type owl:Class ;
                                                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://dbpedia.org/property/year> ;
                                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                    owl:onDataRange xsd:integer
                                                                  ] ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :hasName ;
                                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                    owl:onDataRange xsd:string
                                                                  ] ;
                                                  rdfs:comment "Wine region means, within a single country: a state, province, territory or similar administrative or political subdivision; or a single delimited grape growing area; or a grape growing region or locality as defined in the exporting Party’s laws, regulations, or requirements."@en ;
                                                  rdfs:isDefinedBy "http://dbpedia.org/resource/Category:"@en ;
                                                  rdfs:label "Wine Region"@en .


###  http://schema.org/Winery
<http://schema.org/Winery> rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty :hasWine ;
                                             owl:someValuesFrom :Wine
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://dbpedia.org/ontology/wikiPageID> ;
                                             owl:someValuesFrom xsd:integer
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :hasName ;
                                             owl:someValuesFrom xsd:string
                                           ] ;
                           rdfs:comment "A place where wine is made, especially from grapes that are grown there."@en ;
                           rdfs:isDefinedBy "http://schema.org"@en ;
                           rdfs:label "Winery"@en .


###  https://saref.etsi.org/core/Currency
<https://saref.etsi.org/core/Currency> rdf:type owl:Class ;
                                       rdfs:subClassOf <https://saref.etsi.org/core/UnitOfMeasure> ;
                                       rdfs:comment "The class of units of measure for price" ;
                                       rdfs:isDefinedBy "https://saref.etsi.org/core/" ;
                                       rdfs:label "Currency" .


###  https://saref.etsi.org/core/UnitOfMeasure
<https://saref.etsi.org/core/UnitOfMeasure> rdf:type owl:Class ;
                                            rdfs:comment "The unit of measure is a standard for measurement of a quantity, such as a Property. For example, Power is a property and Watt is a unit of power that represents a definite predetermined power: when we say 10 Watt, we actually mean 10 times the definite predetermined power called \"watt\". Our definition is based on the definition of unit of measure in the Ontology of units of Measure (OM). We propose here a list of some units of measure that are relevant for the purpose of SAREF, but this list can be extended, also using some other ontologies rather than the Ontology of units of Measure (OM)."@en ;
                                            rdfs:isDefinedBy "https://saref.etsi.org/core/"@en ;
                                            rdfs:label "Unit Of Measure"@en .


###  https://w3id.org/winecatalogue#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf <http://dbpedia.org/ontology/Country> ;
         rdfs:comment "An area of land that has its own government, army, etc"@en ;
         rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
         rdfs:label "Country"@en .


###  https://w3id.org/winecatalogue#Good
:Good rdf:type owl:Class ;
      rdfs:subClassOf :WineQuality ;
      rdfs:comment "A concept that refers to good wines"@en ;
      rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
      rdfs:label "Good"@en .


###  https://w3id.org/winecatalogue#Poor
:Poor rdf:type owl:Class ;
      rdfs:subClassOf :WineQuality ;
      rdfs:comment "A concept that represnts fair wines"@en ;
      rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
      rdfs:label "Poor"@en .


###  https://w3id.org/winecatalogue#Price
:Price rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty <https://saref.etsi.org/core/unitOfMeasure> ;
                         owl:allValuesFrom <https://saref.etsi.org/core/Currency>
                       ] ;
       rdfs:comment "The quantity of one thing and especially money that is exchanged or demanded in exchange for another"@en ;
       rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
       rdfs:label "Price"@en .


###  https://w3id.org/winecatalogue#RedWine
:RedWine rdf:type owl:Class ;
         rdfs:subClassOf :WineColor .


###  https://w3id.org/winecatalogue#RoseWine
:RoseWine rdf:type owl:Class ;
          rdfs:subClassOf :WineColor .


###  https://w3id.org/winecatalogue#VeryGood
:VeryGood rdf:type owl:Class ;
          rdfs:subClassOf :WineQuality ;
          rdfs:comment "A concept that refers to an excelent wine"@en ;
          rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
          rdfs:label "Very Good"@en .


###  https://w3id.org/winecatalogue#VeryPoor
:VeryPoor rdf:type owl:Class ;
          rdfs:subClassOf :WineQuality ;
          rdfs:comment "A concept that refers to a not desired wine quality"@en ;
          rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
          rdfs:label "Very Poor"@en .


###  https://w3id.org/winecatalogue#WhiteWine
:WhiteWine rdf:type owl:Class ;
           rdfs:subClassOf :WineColor .


###  https://w3id.org/winecatalogue#Wine
:Wine rdf:type owl:Class ;
      rdfs:subClassOf <http://dbpedia.org/ontology/Wine> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasWineType ;
                        owl:allValuesFrom :WineType
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :isWineOf ;
                        owl:allValuesFrom <http://schema.org/Winery>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasPrice ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Price
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasName ;
                        owl:someValuesFrom xsd:string
                      ] ;
      rdfs:comment "An alcoholic drink that is usually made from grapes, but can also be made from other fruits or flowers. It is made by fermenting the fruit with water and sugar"@en ;
      rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
      rdfs:label "Wine"@en .


###  https://w3id.org/winecatalogue#WineBody
:WineBody rdf:type owl:Class ;
          rdfs:subClassOf :WineTaste ;
          rdfs:comment "The Wine body refers to the richness and weight of the wine in your mouth. Other drinks have a body as well. Take milk, for example. It's not difficult to use fat levels to tell the difference between skim and whole milk."@en ;
          rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
          rdfs:label "Wine Body"@en .


###  https://w3id.org/winecatalogue#WineColor
:WineColor rdf:type owl:Class ;
           rdfs:subClassOf :WineDescriptor ;
           owl:disjointWith :WineTaste ;
           rdfs:comment "The color of wine indicates age, grape variety, density of flavor, acidity and more"@en ;
           rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
           rdfs:label "Wine Color"@en .


###  https://w3id.org/winecatalogue#WineDescriptor
:WineDescriptor rdf:type owl:Class ;
                rdfs:comment "The use of wine tasting descriptors allows the taster to qualitatively relate the aromas and flavors that the taster experiences and can be used in assessing the overall quality of wine"@en ;
                rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
                rdfs:label "Wine Descriptor"@en .


###  https://w3id.org/winecatalogue#WineFlavour
:WineFlavour rdf:type owl:Class ;
             rdfs:subClassOf :WineTaste ;
             rdfs:comment "Flavor refers to the taste of a wine in your mouth. As well as reflecting the aromas absorbed retro-nasally, the overall flavor of a wine is also influenced by the wine’s acidity, sweetness, alcohol level, tannins, astringency, body and in sparkling wines by its fizziness, as these components can accentuate or neutralize the flavors."@en ;
             rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
             rdfs:label "Wine Flavour"@en .


###  https://w3id.org/winecatalogue#WineQuality
:WineQuality rdf:type owl:Class ;
             rdfs:comment "Wine quality refers to the factors that go into producing a wine, as well as the indicators or characteristics that tell you if the wine is of high quality. When you know what influences and signifies wine quality, you'll be in a better position to make good purchases"@en ;
             rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
             rdfs:label "Wine Quality"@en .


###  https://w3id.org/winecatalogue#WineSugar
:WineSugar rdf:type owl:Class ;
           rdfs:subClassOf :WineTaste ;
           rdfs:comment "The sugars in wine grapes are what make winemaking possible. During the process of fermentation, sugars are broken down and converted by yeasts into ethanol alcohol and carbon dioxide. Grapes accumulate sugars as they grow on the grapevine through the translocation of sucrose molecules that are produced by photosynthesis from the leaves. During ripening the sucrose molecules are hydrolyzed (inverted) by the enzyme invertase into glucose and fructose. By the time of harvest, between 15-25% of the grape will be composed of simple sugars. Both glucose and fructose are six-carbon sugars but three, four, five and seven-carbon sugars are also present in the grape."@en ;
           rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
           rdfs:label "Wine Sugar"@en .


###  https://w3id.org/winecatalogue#WineTaste
:WineTaste rdf:type owl:Class ;
           rdfs:subClassOf :WineDescriptor ;
           rdfs:comment "A concept that represents the flvour of teh wine."@en ;
           rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
           rdfs:label "Wine Taste"@en .


###  https://w3id.org/winecatalogue#WineType
:WineType rdf:type owl:Class ;
          rdfs:comment "The wine type refers to the wine variety"@en ;
          rdfs:isDefinedBy "https://w3id.org/winecatalogue#"@en ;
          rdfs:label "Wine Type"@en .


